package lessons;

// date 9/22
public class Question1 {
	public static void main(String[] args) {
		int x = 7;

		if (x < 10) {
			System.out.println("Less than 10");
		}
		
		int y = new Integer(15);
		
		if(y < 10) {
			System.out.println("Less than 10");
		} else if (y > 10) {
			System.out.println("Greater than 10");
		}
	}
}
