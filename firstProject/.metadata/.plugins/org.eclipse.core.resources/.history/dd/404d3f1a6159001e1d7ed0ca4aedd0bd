package lessons;

// date 9/22
public class assignments {
	public static void main(String[] args) {
		int x = 7;

		if (x < 10) {
			System.out.println("Less than 10");
		}
		
		Integer x = new Integer(15);
		
		if(x < 10) {
			System.out.println("Less than 10");
		} else if (x > 10) {
			System.out.println("Greater than 10");
		}
	}
}
