package looping;

public class WhileLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		boolean condition = true;
		
		int number = 1;
		
		while(number <= 12) {
			System.out.print(number);
			//for (int i = 1; i <= 12; i++) {
				//System.out.print(i * number);
			//}
			System.out.print("\n");
			number = number ++;
		}
	}

}
