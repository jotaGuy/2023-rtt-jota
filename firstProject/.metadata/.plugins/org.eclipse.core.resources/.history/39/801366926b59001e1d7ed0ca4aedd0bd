package controlflow;

public class questionOne {
	public static void main(String[] args) {
		int x = 7;

		if (x < 10) {
			System.out.println("Less than 10");
		}

		int y = 15;

		if (y < 10) {
			System.out.println("Less than 10");
		} else if (y > 10) {
			System.out.println("Greater than 10");
		}
	}
}
